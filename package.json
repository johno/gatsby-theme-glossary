{
  "private": true,
  "name": "gatsby-theme-glossary",
  "version": "0.0.1",
  "scripts": {
    "start": "yarn workspace gatsby-starter-glossary start",
    "build": "gatsby build",
    "docs": "gatsby develop",
    "now-build": "yarn build",
    "publish": "lerna publish",
    "publish:ci": "lerna publish -y --canary --preid ci --dist-tag=ci --force-publish=*",
    "test": "yarn build"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "dependencies": {
    "gatsby": "2.13.18",
    "lerna": "3.15.0",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  }
}
